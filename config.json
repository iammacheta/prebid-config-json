            "conversionRateFile": "https://cdn.jsdelivr.net/gh/prebid/currency-file@1/latest.json"
        },
        "userSync": {
            "filterSettings": {
                "iframe": {
                    "bidders": "*",
                    "filter": "include"
                }
            },
            "encryptedSignalSources": {
                "sources": [
                    {
                        "source": [
                            "uidapi.com",
                            "sharedid.org",
                            "criteo.com"
                        ],
                        "encrypt": false
                    }
                ],
                "registerDelay": 3000
            },
            "userIds": [
                {
                    "name": "sharedId",
                    "storage": {
                        "type": "cookie",
                        "name": "_sharedid",
                        "expires": 365
                    }
                },
                {
                    "name": "criteo"
                }
            ]
        }
    },
